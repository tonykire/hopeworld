#include <iostream>
using namespace std;

int proper_fraction (int n) {
    int res = n, del = 2;
    while (n != 1) {
        if (n % del == 0) {
            while (n % del == 0) {
                n /= del;
            }
            res *= (del - 1);
            res /= del;
        }
        del++;
    }
    return res;
}

int main ()
{
	int n;
	cin >> n;

    cout << proper_fraction(n);

	return 0;
}
